{
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "buildCommand": "rm -rf node_modules package-lock.json && npm install && npm run build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
