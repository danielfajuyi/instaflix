{
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "buildCommand": "rm -rf package-lock.json && npm install && npm run build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://instaflix-api.vercel.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
